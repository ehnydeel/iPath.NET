@namespace iPath.Blazor.Componenents

@inject ServiceRequestViewModel vm

<iPath.LHCForms.LhcForm ReadOnly="true" Questionnaire="@q" QuestionnaireResponse="@Data.Resource" />

@code {
    [Parameter, EditorRequired]
    public QuestionnaireResponseData Data { get; set; }

    string? q;

    protected override async Task OnInitializedAsync()
    {
        q = await vm.GetQuesiotnnaireResource(Data.QuestionnaireId, Data.Version);
    }
}
